{
    "description": "Fully funtiontioning express, react, redux application with unit and e2e tests stubbed app",
    "author": "Eddie Feather",
    "name": "basic-react-redux-node",
    "version": "0.1.0",
    "private": true,
    "main": "./dist/server/index.js",
    "scripts": {
        "dev": "webpack-dev-server --mode development  --open --hot --config ./webpack.config-dev",
        "format": "prettier --write \"./**/*.{ts*,js*,md,json,css}\" || echo \"Unable to format.  continuing anyway.\"",
        "build-server": "tsc",
        "build-client": "webpack --mode production",
        "build": "npm run format && npm run build-server && npm run build-client",
        "start": "npm run build && node ./dist/server/index.js",
        "test": "mocha -r ts-node/register src/test/unit/**/*.spec.ts",
        "e2e-test": "start-server-and-test start http://localhost:8080/basicapp e2e-test-gui",
        "e2e-test-api": "cucumber-js  --profile api",
        "e2e-test-gui": "cucumber-js  --profile gui",
        "_e2e-test": "npm run e2e-test-api && npm run e2e-test-gui"
    },
    "dependencies": {
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.6",
        "@types/express-ws": "^3.0.0",
        "@types/ws": "^7.2.4",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "express-ws": "^4.0.0",
        "http-shutdown": "^1.2.2",
        "http-status-codes": "^1.4.0",
        "react-hot-loader": "^4.13.1",
        "redux": "^4.0.5",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/cucumber": "^6.0.1",
        "@types/mocha": "^7.0.2",
        "@types/react": "16.9.23",
        "@types/react-dom": "16.9.5",
        "@types/react-redux": "^7.1.7",
        "@types/react-router": "^5.1.4",
        "@types/selenium-webdriver": "^4.0.9",
        "chai": "^4.2.0",
        "chromedriver": "^80.0.2",
        "connected-react-router": "^6.7.0",
        "cucumber": "^6.0.5",
        "cucumber-pretty": "^6.0.0",
        "html-webpack-plugin": "^5.5.0",
        "mocha": "^7.1.1",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.2.0",
        "react-router": "^5.1.2",
        "redux-devtools-extension": "^2.13.8",
        "selenium": "^2.20.0",
        "selenium-webdriver": "^3.6.0",
        "start-server-and-test": "^1.10.11",
        "ts-loader": "^9.4.2",
        "ts-node": "^8.8.1",
        "typescript": "^3.8.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^3.11.0"
    }
}
