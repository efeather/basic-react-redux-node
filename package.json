{
    "name": "basic-react-redux-node",
    "version": "0.1.0",
    "private": true,
    "main": "./dist/server/index.js",
    "scripts": {
        "format": "prettier --write \"./**/*.{ts*,js*,md,json,css}\" || echo \"Unable to format.  continuing anyway.\"",
        "build-server": "tsc",
        "build-client": "webpack",
        "build": "npm run format && npm run build-server && npm run build-client",
        "start": "npm run build && node ./dist/server/index.js",
        "test": "mocha -r ts-node/register src/test/unit/**/*.spec.ts",
        "e2e-test": "npm run e2e-test-api && npm run e2e-test-gui",
        "e2e-test-api": "cucumber-js  --profile api",
        "e2e-test-gui": "cucumber-js  --profile gui"
    },
    "dependencies": {
        "@types/compression": "^1.7.0",
        "@types/express": "github:types/express",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "http-status-codes": "^1.4.0",
        "react-scripts": "3.4.0",
        "redux": "^4.0.5",
        "redux-thunk": "2.3.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/cucumber": "^6.0.1",
        "@types/mocha": "^7.0.2",
        "@types/react": "16.9.23",
        "@types/react-dom": "16.9.5",
        "@types/react-redux": "^7.1.7",
        "@types/react-router": "^5.1.4",
        "@types/selenium-webdriver": "^4.0.9",
        "chai": "^4.2.0",
        "chromedriver": "^80.0.2",
        "connected-react-router": "^6.7.0",
        "cucumber": "^6.0.5",
        "cucumber-pretty": "^6.0.0",
        "mocha": "^7.1.1",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-redux": "^7.2.0",
        "react-router": "^5.1.2",
        "redux-devtools-extension": "^2.13.8",
        "selenium": "^2.20.0",
        "selenium-webdriver": "^3.6.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.8.1",
        "tslint": "^6.0.0",
        "typescript": "^3.8.3",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11"
    }
}
